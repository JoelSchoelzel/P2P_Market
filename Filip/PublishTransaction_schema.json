{
  "$defs": {
    "CreatedDateTime": {
      "description": "The time of prediction for next hour, it can also identify the bids or transactions in hours.",
      "properties": {
        "time": {
          "title": "Time",
          "type": "string"
        }
      },
      "required": [
        "time"
      ],
      "title": "CreatedDateTime",
      "type": "object"
    },
    "MarketRole": {
      "description": "This version of Role is currently only for implementing in bid",
      "enum": [
        "buyer",
        "seller"
      ],
      "title": "MarketRole",
      "type": "string"
    },
    "PowerDirection": {
      "properties": {
        "tradingObjectRole": {
          "$ref": "#/$defs/MarketRole"
        },
        "tradingObjectID": {
          "description": "...",
          "title": "Tradingobjectid",
          "type": "string"
        }
      },
      "required": [
        "tradingObjectRole",
        "tradingObjectID"
      ],
      "title": "PowerDirection",
      "type": "object"
    },
    "Price": {
      "description": "The price of the buying or selling will be at first in bids provided from every building. But the final transacted\nprice will be by coordinator with multiple trading round determined.",
      "properties": {
        "price": {
          "title": "Price",
          "type": "number"
        }
      },
      "required": [
        "price"
      ],
      "title": "Price",
      "type": "object"
    },
    "Quantity": {
      "description": "Quantity will be at first in bids provided. After matching among buyers and sellers, appropriate matches will have\na certain quantity to exchange. One seller can sell its energy to one or more buyers.",
      "properties": {
        "quantity": {
          "title": "Quantity",
          "type": "number"
        }
      },
      "required": [
        "quantity"
      ],
      "title": "Quantity",
      "type": "object"
    },
    "TradeInformation": {
      "description": "The results of transaction",
      "properties": {
        "realPrice": {
          "allOf": [
            {
              "$ref": "#/$defs/Price"
            }
          ],
          "description": "..."
        },
        "realQuantity": {
          "allOf": [
            {
              "$ref": "#/$defs/Quantity"
            }
          ],
          "description": "..."
        },
        "powerDirection": {
          "allOf": [
            {
              "$ref": "#/$defs/PowerDirection"
            }
          ],
          "description": "..."
        }
      },
      "required": [
        "realPrice",
        "realQuantity",
        "powerDirection"
      ],
      "title": "TradeInformation",
      "type": "object"
    },
    "TradeResults": {
      "properties": {
        "tradeResults": {
          "items": {
            "$ref": "#/$defs/TradeInformation"
          },
          "title": "Traderesults",
          "type": "array"
        }
      },
      "required": [
        "tradeResults"
      ],
      "title": "TradeResults",
      "type": "object"
    }
  },
  "description": "Represents transaction of power direction and negotiated price and quantity",
  "properties": {
    "transactionID": {
      "description": "...",
      "title": "Transactionid",
      "type": "string"
    },
    "transactionCreatedDateTime": {
      "allOf": [
        {
          "$ref": "#/$defs/CreatedDateTime"
        }
      ],
      "description": "Date and time that this Bid was created"
    },
    "tradeResults": {
      "anyOf": [
        {
          "$ref": "#/$defs/TradeResults"
        },
        {
          "items": {},
          "type": "array"
        }
      ],
      "default": null,
      "description": "...",
      "title": "Traderesults"
    },
    "refMarketParticipant": {
      "description": "...",
      "title": "Refmarketparticipant",
      "type": "string"
    }
  },
  "required": [
    "transactionID",
    "transactionCreatedDateTime",
    "refMarketParticipant"
  ],
  "title": "PublishTransaction",
  "type": "object"
}